    "next": "14.0.4",
    "open": "^10.0.0",
    "react": "^18",
    "react-dom": "^18",
    "sharp": "^0.34.5",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4",
    "zustand": "^5.0.8"
