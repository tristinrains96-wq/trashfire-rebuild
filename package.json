{
  "name": "trashfire",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "clean": "node -e \"try{require('fs').rmSync('.next',{recursive:true,force:true})}catch(e){}\"",
    "dev:open": "concurrently -k \"pnpm dev || npm run dev\" \"pnpm _open_when_ready || npm run _open_when_ready\"",
    "_open_when_ready": "wait-on http://localhost:3000/healthz && node scripts/open.js",
    "smoke": "node scripts/smoke.mjs",
    "phase2_5:check": "npx tsx scripts/run_phase2_5_check.ts",
    "phase2_5:test": "npx ts-node scripts/phase2_5_test.ts",
    "ffmpeg:install": "powershell -ExecutionPolicy Bypass -File scripts/install_ffmpeg_windows.ps1",
    "public:scan": "npx tsx scripts/public_secret_scan.ts",
    "public:check": "npx tsx scripts/public_check.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
